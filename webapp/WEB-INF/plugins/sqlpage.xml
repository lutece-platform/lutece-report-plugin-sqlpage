<?xml version="1.0" encoding="UTF-8" standalone="no"?><plug-in>
    <name>sqlpage</name>
    <class>fr.paris.lutece.plugins.sqlpage.service.SQLPagePlugin</class>
    <version>1.0.1-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>sqlpage.plugin.description</description>
    <provider>sqlpage.plugin.provider</provider>
    <provider-url>http://dev.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2015 City of Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>
    
    
    <javascript-files>
         <javascript-file>sqlpage/Chart.min.js</javascript-file>
    </javascript-files>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>SQLPAGE_MANAGEMENT</feature-id>
            <feature-title>sqlpage.adminFeature.ManageSQLPage.name</feature-title>
            <feature-description>sqlpage.adminFeature.ManageSQLPage.description</feature-description>
              <feature-level>0</feature-level>                    
            <feature-url>jsp/admin/plugins/sqlpage/ManageSQLPages.jsp</feature-url>
            <feature-icon-url/>
      </admin-feature>
       <admin-feature>
            <feature-id>SQLPAGE_MANAGEMENT_WEBMASTER</feature-id>
            <feature-title>sqlpage.adminFeature.ManageSecondaireSQLPage.name</feature-title>
            <feature-description>sqlpage.adminFeature.ManageSecondaireSQLPage.description</feature-description>
              <feature-level>2</feature-level>                    
            <feature-url>jsp/admin/plugins/sqlpage/ManageSecondaireSQLPages.jsp</feature-url>
            <feature-icon-url/>
      </admin-feature>
    </admin-features>   
   
    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>sqlpage</application-id>
            <application-class>fr.paris.lutece.plugins.sqlpage.web.SQLPageApp</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
   </portlets>
</plug-in>